<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="members.datamodel.AccountRoleInfo" table="account_role_info">
  		<id name="accountRoleId" column="account_role_id">
      	<generator class="native"/>
 		</id>	 		 		
   	<property name="accountRoleName" column="account_role_name" length="20"/>
    </class>
</hibernate-mapping>
